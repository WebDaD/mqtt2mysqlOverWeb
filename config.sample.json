{
  "key":"my-secret-key",
  "sender": {
    "broker": {
      "host": "server",
      "port": 1883,
      "username": "user",
      "password": "pwd"
    },
    "post": {
      "host": "http://localhost"
    }
  },
  "receiver": {
    "port":3000,
    "database":{
      "host"     : "localhost",
      "user"     : "mqtt",
      "password" : "mqtt",
      "database" : "mqtt"
    },
    "store":"/var/mqtt2mysqlOverWeb/files/",
    "ignoreInsertError": true
  },
  "topics": [
    {
      "topic":"some/nice/topic",
      "table":"nice"
    },
    {
      "topic":"some/real/awesome/topic",
      "table":"real"
    }
  ],
  "structure": {
    "fields": [
      {
        "field":"timestamp",
        "type":"DATETIME"
      },
      {
        "field":"title",
        "type":"VARCHAR(255)"
      }
    ],
    "indices": [
      "timestamp"
    ],
    "primary": [
      "timestamp",
      "title"
    ],
    "partioning": {
      "on":"timestamp",
      "count":10
    },
    "files" : [
      {
        "name":"image",
        "extension":"jpg",
        "folder":"/mnt/capri/images",
        "id":"title"
      }
    ]
  },
  "filter": [
    {
      "field":"title",
      "operator":"equals",
      "values": [
        "some",
        "any"
      ]
    }
  ]
}